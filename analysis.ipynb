    "import pandas as pd\n",
    "\n",
    "LOGS = \"stopwatch.csv\"\n",
    "df = pd.read_csv(LOGS)\n",
    "# average time and variance, standard deviation\n",
    "res = df.groupby(\"src\")[\"time\"].agg(['mean', 'var', 'std']).reset_index()\n",
    "res"
   ]
